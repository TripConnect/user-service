  "devDependencies": {
    "@types/bcrypt": "^5.0.2",
    "@types/bcryptjs": "^2.4.6",
    "@types/node": "^22.13.11",
    "@types/uuid": "^9.0.8",
    "npm-check": "^6.0.1",
    "sequelize-cli": "^6.6.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "tslib": "^2.8.1",
    "typescript": "^5.4.5"
  }
}
