{
  "name": "user-service",
  "version": "1.0.0",
  "description": "user-service",
  "main": "application.ts",
  "dependencies": {
    "@grpc/grpc-js": "^1.10.2",
    "@grpc/grpc-js-xds": "^1.10.0",
    "@grpc/proto-loader": "^0.6.0",
    "@grpc/reflection": "^1.0.0",
    "@type-cacheable/core": "^14.1.0",
    "@type-cacheable/node-cache-adapter": "^14.1.0",
    "async": "^1.5.2",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^2.4.3",
    "common-utils": "github:TripConnect/common-utils",
    "dotenv": "^16.4.5",
    "google-protobuf": "^3.0.0",
    "kafkajs": "^2.2.4",
    "lodash": "^4.6.1",
    "minimist": "^1.2.0",
    "module-alias": "^2.2.3",
    "mysql2": "^3.9.4",
    "node-proto-lib": "github:TripConnect/node-proto-lib",
    "sequelize": "^6.37.3",
    "winston": "^3.13.0"
  },
  "scripts": {
    "test": "tsc && tsc-alias && node dist/application.js",
    "build": "tsc && tsc-alias",
    "docker:build": "docker build -t user-service:latest .",
    "docker:run": "docker run --name user-service -p 31072:31072 user-service:latest",
    "start": "ts-node src/application.ts",
    "migrate:gen": "npx sequelize-cli model:generate --name Data --attributes name:string",
    "migrate:up": "npx sequelize-cli db:migrate",
    "migrate:reset": "npx sequelize-cli db:migrate:undo:all",
    "seed:up": "npx sequelize-cli db:seed:all",
    "seed:reset": "npx sequelize-cli db:seed:undo:all"
  },
